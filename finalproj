'''Required filetypes: h5ad, mtx, rds?, tsv?, fastq?, bcl?, h5?, hdf5?, csv?'''
import scanpy as sc
class FileConverter:
    def __init__(self, infile, outfile):
        if self.filetypeidentifier(infile) == 'mtx' and self.filetypeidentifier(outfile) == 'h5ad':
            pass
            #call correct method
    def matrix_to_h5ad(self, mtxfile, h5adfile):
        inf = sc.read_mtx(mtxfile)
        outf = h5adfile.write(inf)
        return outf
    def filetypeidentifier(self, file): #necessary?
        pass
